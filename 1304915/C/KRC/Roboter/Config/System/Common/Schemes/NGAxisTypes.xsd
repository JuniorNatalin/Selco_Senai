<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xml:lang="EN">
   <xs:simpleType name="simulationType">
      <xs:annotation>
         <xs:documentation>On: Async simu drivers and CF default simu filter
On_Silent: Like On, but without HMI-message
Off: Normal operation with real HW
Deactivated: Allways operation with real HW. Overrides implicit drive simu in office-mode</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="On"/>
         <xs:enumeration value="On_Silent"/>
         <xs:enumeration value="Off"/>
         <xs:enumeration value="Deactivated"/>
      </xs:restriction>
   </xs:simpleType>
</xs:schema>
